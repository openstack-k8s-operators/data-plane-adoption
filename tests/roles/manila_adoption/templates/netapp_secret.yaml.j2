---
apiVersion: v1
kind: Secret
metadata:
  name: osp-secret-manila-netapp
  namespace: openstack
stringData:
  netapp-secrets.conf : |
    [netapp]
    netapp_server_hostname = {{ manila_netapp_config.netapp_server_hostname }}
    netapp_login = {{ manila_netapp_config.netapp_login }}
    netapp_password = {{ manila_netapp_config.netapp_password }}
    netapp_vserver = {{ manila_netapp_config.netapp_vserver }}
